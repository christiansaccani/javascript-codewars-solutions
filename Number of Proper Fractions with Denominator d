function properFractions(n) {
  if (n <= 1) return 0;

  //For a given positive integer n, Totient Function is the count of integers from 1 to n
  //that are coprime with n. Formula: n * (1 - 1/p1) * (1 - 1/p2) * ... * (1 - 1/pk)

  let result = n;
  let p = 2;

  while (p * p <= n) {
    if (n % p === 0) {
      while (n % p === 0) {
        n /= p;
      }
      result -= result / p;
    }
    p++;
  }

  if (n > 1) {
    result -= result / n;
  }
  return result;
}
